./gradlew clean &&
echo "Build clean" &&

SPRING_PROFILES_ACTIVE=prod ./gradlew clean build --no-build-cache &&
SPRING_PROFILES_ACTIVE=prod ./gradlew build -x test &&
echo "Build finished" &&

docker build --no-cache --build-arg DEPENDENCY=build/dependency -t jeekimdev/jocr --platform linux/amd64 -f ./Dockerfile . &&
docker push jeekimdev/jocr
